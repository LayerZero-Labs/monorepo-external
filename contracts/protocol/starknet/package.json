{
    "name": "@layerzerolabs/protocol-starknet-v2",
    "version": "0.0.0",
    "private": false,
    "description": "Starknet implementation of LayerZero protocol v2",
    "license": "MIT",
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.cjs",
            "import": "./dist/index.js",
            "default": "./dist/index.cjs"
        },
        "./cairo/layerzero": "./layerzero",
        "./cairo/libs/lz_utils": "./libs/lz_utils",
        "./cairo/libs/enumerable_set": "./libs/enumerable_set",
        "./cairo/libs/multisig": "./libs/multisig"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "build": "lz-tool scarb --release build && pnpm build:abi && pnpm build:verification && tsup",
        "build:abi": "lz-tool extra starknet build-typescript-sdk layerzero target src/generated",
        "build:verification": " generate-starknet-verification --package layerzero --path src/generated/verification",
        "clean": "rm -rf ./node_modules .turbo ./dist && pnpm generated:clean",
        "format": "lz-tool scarb fmt",
        "format:check": "node --run format --check",
        "generated:clean": "rm -rf ./target .snfoundry_cache ./src/generated",
        "lint": "pnpm format:check && lz-tool scarb lint --test",
        "test": "lz-tool scarb test",
        "update:contracts": "tools/update_contracts.sh"
    },
    "devDependencies": {
        "@layerzerolabs/build-utils-starknet": "workspace:*",
        "@layerzerolabs/tsup-configuration": "workspace:*",
        "@layerzerolabs/typescript-configuration": "workspace:*",
        "@layerzerolabs/vm-tooling-starknet": "workspace:*",
        "tsup": "catalog:"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    }
}
