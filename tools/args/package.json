{
    "name": "@layerzerolabs/args",
    "version": "0.0.0",
    "private": false,
    "license": "MIT",
    "type": "module",
    "exports": {
        "types": "./dist/index.d.ts",
        "require": "./dist/index.cjs",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
    },
    "main": "./dist/index.cjs",
