{
    "name": "@layerzerolabs/depcheck",
    "version": "0.0.0",
    "private": false,
    "license": "MIT",
    "type": "module",
    "exports": {
        "types": "./dist/index.d.ts",
        "require": "./dist/index.cjs",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "depcheck": "./dist/run.js"
    },
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "build": "tsup",
        "depcheck": "tsx ./src/run.ts",
        "lint": "eslint . --max-warnings 0 || (eslint . --fix --max-warnings 0 && false)",
        "test": "vitest --run --pass-with-no-tests --typecheck"
    },
    "dependencies": {
        "commander": "catalog:",
        "depcheck": "catalog:",
        "handlebars": "catalog:",
        "ignore": "catalog:",
        "isomorphic-git": "catalog:",
        "js-yaml": "catalog:",
        "p-limit": "catalog:"
    },
    "devDependencies": {
        "@layerzerolabs/tsup-configuration": "workspace:*",
        "@layerzerolabs/typescript-configuration": "workspace:*",
        "@types/js-yaml": "catalog:",
        "@types/node": "catalog:",
        "tsup": "catalog:",
        "tsx": "catalog:",
        "typescript": "catalog:",
        "vitest": "catalog:"
    },
    "publishConfig": {
        "access": "restricted",
        "registry": "https://registry.npmjs.org/"
    }
}
